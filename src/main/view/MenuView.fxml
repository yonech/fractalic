<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.input.KeyCodeCombination?>

<MenuBar fx:id="mainMenuBar" prefHeight="29.0" prefWidth="318.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.control.menu.MainMenuController">
  <menus>
      <Menu fx:id="menuElFile" mnemonicParsing="false" text="File">
         <items>
            <MenuItem fx:id="menuOptFileNew" mnemonicParsing="false" text="New" />
            <MenuItem fx:id="menuOptFileOpen" mnemonicParsing="false" text="Open" />
            <MenuItem id="MenuFileSave" fx:id="menuOptFileSave" mnemonicParsing="false" text="Save">
               <accelerator>
                  <KeyCodeCombination alt="UP" code="S" control="ANY" meta="UP" shift="UP" shortcut="UP" />
               </accelerator>
            </MenuItem>
            <MenuItem id="MenuFileSaveAs" fx:id="menuOptFileSaveAs" mnemonicParsing="false" text="Save As...">
               <accelerator>
                  <KeyCodeCombination alt="UP" code="S" control="ANY" meta="UP" shift="ANY" shortcut="UP" />
               </accelerator>
            </MenuItem>
            <MenuItem id="MenuFileSaveAs" fx:id="menuOptFileProperties" mnemonicParsing="false" text="Properties" />
            <MenuItem fx:id="menuOptFileClose" mnemonicParsing="false" text="Close" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Edit">
         <items>
            <MenuItem fx:id="menuOptUndo" mnemonicParsing="false" text="Undo">
               <accelerator>
                  <KeyCodeCombination alt="UP" code="Z" control="ANY" meta="UP" shift="UP" shortcut="UP" />
               </accelerator>
            </MenuItem>
            <MenuItem fx:id="menuOptRedo" mnemonicParsing="false" text="Redo">
               <accelerator>
                  <KeyCodeCombination alt="UP" code="Z" control="ANY" meta="UP" shift="ANY" shortcut="UP" />
               </accelerator>
            </MenuItem>
            <MenuItem fx:id="menuOptEditClear" mnemonicParsing="false" text="Clear" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Select">
        <items>
          <MenuItem fx:id="menuOptToolsExample" mnemonicParsing="false" text="Fractal" />
        </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Tools">
         <items>
            <MenuItem mnemonicParsing="false" text="Fractal draw" />
            <MenuItem mnemonicParsing="false" text="Paintbrush" />
            <MenuItem mnemonicParsing="false" text="Fill" />
         </items>
      </Menu>
      <Menu mnemonicParsing="false" text="Help">
         <items>
            <MenuItem mnemonicParsing="false" text="About" />
         </items>
      </Menu>
  </menus>
</MenuBar>
