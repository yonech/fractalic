<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.control.menu.examples.ExamplesController">
   <children>
      <VBox alignment="BOTTOM_CENTER" minHeight="-Infinity" minWidth="-Infinity" spacing="20.0">
         <children>
            <VBox fx:id="examplesList" prefHeight="200.0" prefWidth="100.0" spacing="10.0">
               <VBox.margin>
                  <Insets left="20.0" top="20.0" />
               </VBox.margin>
            </VBox>
            <Button fx:id="examplesSelect" mnemonicParsing="false" text="Select">
               <VBox.margin>
                  <Insets bottom="20.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <Separator orientation="VERTICAL" prefHeight="200.0" />
      <Canvas fx:id="canvas" height="400.0" width="500.0" />
   </children>
</HBox>
